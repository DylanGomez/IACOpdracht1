    <soapenv:Header/>
    <soapenv:Body>
        <acc:AccuLadenRequest>
            <Vermogen>10</Vermogen>
            <Duur>20</Duur>
            <procent>20</procent>
        </acc:AccuLadenRequest>
    </soapenv:Body>
</soapenv:Envelope>

<!--Response 1-->

<S:Envelope xmlns:S="http://schemas.xmlsoap.org/soap/envelope/">
<S:Body>
    <ns4:AccuLadenResponse xmlns:ns2="http://dylangomezvazquez/AccuLadenError" xmlns:ns3="http://dylangomezvazquez/AccuLadenRequest" xmlns:ns4="http://dylangomezvazquez/AccuLadenResponse">
        <result>De accu heeft heeft een vermogen van 10 Ampere,
            de accu heeft momenteel nog 20 procent over opladen bij het huidige tankstation duurt: 20 minuten</result>
    </ns4:AccuLadenResponse>
</S:Body>
</S:Envelope>

